<layout>
    <default>
        <reference name="head">
            <action method="addItem"><type>skin_css</type><name>css/fonts.css</name></action>
            <action method="addItem"><type>skin_js</type><name>js/html5shiv.js</name></action>
            <action method="addItem"><type>skin_js</type><name>js/selectivizr-min.js</name></action>
            <action method="addItem"><type>skin_js</type><name>js/prototype.formalize.min.js</name></action>
        </reference>

        <remove name="right.permanent.callout" />
		<remove name="left.permanent.callout" />
		<remove name="paypal.partner.right.logo" />
    </default>

    <contacts_index_index>
		<reference name="head">
		    <action method="setTitle"><title>Contact Us</title></action>
		</reference>
		<reference name="breadcrumbs">
            <action method="addCrumb">
                <crumbName>cms_page</crumbName>
                <crumbInfo>
                    <label>Contact Us</label>
                    <title>Contact Us</title>
                </crumbInfo>
            </action>
        </reference>
    </contacts_index_index>
</layout>